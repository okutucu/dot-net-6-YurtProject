@{
    ViewData["Title"] = "Room Income";
}

@model List<IncomeDetailDto>

<div class="text-center">
    <h1 class="display-4">Income  Page</h1>
</div>

<div class="my-2">
    <a class="btn btn-primary mr-2" asp-action="Create" asp-controller="RoomIncome">Add New +</a>
</div>

<form class="mt-2" asp-controller="RoomIncome" asp-action="GetBySelected" method="get">

    <div class="card">
        <div class="card-body">
            <div class="row" id="paymentSection">
                <div class="col-6">
                    <input id="monthly" type="month" name="selectedDate" class="form-control">
                </div>
            </div>
        </div>
    </div>



    <div class="form-group">
        <input type="submit" value="Submit" class="btn btn-primary" />
    </div>

</form>
